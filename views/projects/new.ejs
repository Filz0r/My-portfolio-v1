<section class="newProj">
  <div class="container">
    <div class="card bg-dark flex newCard">
      <h1 class="my-2 xxl text-center">New Project</h1>
      <% if (path === '/admin/en/projects/new') { %>
        <form action="/admin/en/projects/new" method="POST" enctype="multipart/form-data">
          <%- include('../partials/_form_fields_projects') %>
        </form>
      <% } else if (path === '/admin/pt/projects/new') { %>
        <form action="/admin/pt/projects/new" method="POST" enctype="multipart/form-data">
          <%- include('../partials/_form_fields_projects') %>
        </form>
      <% } %>
    </div>
  </div>
</section>
